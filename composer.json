    "keywords": [
        "bookin",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/bookin/yii2-dynamicform/issues",
        "wiki": "https://github.com/bookin/yii2-dynamicform/wiki/",
        "source": "https://github.com/bookin/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "Wanderson Bragan√ßa",
            "email": "wanderson.wbc@gmail.com",
            "homepage": "http://wbraganca.com"
        },
        {
            "name": "Anton Mikhailov",
            "email": "issues.bookin@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "symfony/dom-crawler": "dev-master",
        "symfony/css-selector": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "bookin\\dynamicform\\": "src"
        }
    }
}
